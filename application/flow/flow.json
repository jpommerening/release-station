{
   "places": {
      "entry": {
         "redirectTo": "grid"
      },

      "grid/:scope": {
         "page":  "grid",
         "targets": {
            "cancel": "entry",
            "next": "calendar",
            "repository": "history"
         }
      },

      "calendar/:date": {
         "page":  "calendar",
         "targets": {
            "cancel": "entry",
            "previous": "grid",
            "next": "history"
         }
      },

      "history/:owner/:repo/:version": {
         "page":  "history",
         "targets": {
            "cancel": "entry",
            "previous": "calendar"
         }
      }
   }
}
