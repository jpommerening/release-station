{
   "places": {
      "entry": {
         "redirectTo": "calendar"
      },

      "grid": {
         "page":  "grid",
         "targets": {
            "cancel": "entry",
            "next": "calendar"
         }
      },

      "calendar/:date": {
         "page":  "calendar",
         "targets": {
            "cancel": "entry",
            "previous": "grid",
            "next": "timeline"
         }
      },

      "timeline/:repository": {
         "page":  "timeline",
         "targets": {
            "cancel": "entry"
         }
      }
   }
}
